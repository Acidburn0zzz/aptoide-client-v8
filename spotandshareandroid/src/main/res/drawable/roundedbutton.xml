<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


  <item>

    <shape android:shape="oval">
      <stroke
          android:color="#509E9E9E"
          android:width="2dp"/>
      <solid android:color="#50757575"/>
      <size
          android:height="50dp"
          android:width="50dp"/>
    </shape>


  </item>

  <item
      android:bottom="4px"
      android:right="3px">

    <shape android:shape="oval">
      <stroke
          android:color="#00B0C5"
          android:width="0dp"/>
      <solid android:color="#00B0C5"/>
      <size
          android:height="50dp"
          android:width="50dp"/>
    </shape>


  </item>


</layer-list>